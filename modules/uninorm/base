Description:
Base layer for normalization forms of Unicode strings.

Files:
lib/uninorm.in.h
m4/libunistring-base.m4

Depends-on:
unitypes

configure.ac:
gl_LIBUNISTRING_LIBHEADER([0.9.11], [uninorm.h])
AC_PROG_MKDIR_P

Makefile.am:
BUILT_SOURCES += $(LIBUNISTRING_UNINORM_H)

uninorm.h: uninorm.in.h
	$(AM_V_GEN)
@NMD@	$(AM_V_at)$(MKDIR_P) '%reldir%'
	$(AM_V_at){ echo '/* DO NOT EDIT! GENERATED AUTOMATICALLY! */'; \
	  cat $(srcdir)/uninorm.in.h; \
	} > $@-t && \
	mv $@-t $@
MOSTLYCLEANFILES += uninorm.h uninorm.h-t

Include:
"uninorm.h"

