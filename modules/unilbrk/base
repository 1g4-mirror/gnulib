Description:
Base layer for line breaking.

Files:
lib/unilbrk.in.h
m4/libunistring-base.m4

Depends-on:
unitypes
localcharset

configure.ac:
gl_LIBUNISTRING_LIBHEADER([0.9.11], [unilbrk.h])
AC_PROG_MKDIR_P

Makefile.am:
BUILT_SOURCES += $(LIBUNISTRING_UNILBRK_H)

unilbrk.h: unilbrk.in.h
	$(AM_V_GEN)
@NMD@	$(AM_V_at)$(MKDIR_P) '%reldir%'
	$(AM_V_at){ echo '/* DO NOT EDIT! GENERATED AUTOMATICALLY! */'; \
	  cat $(srcdir)/unilbrk.in.h; \
	} > $@-t && \
	mv $@-t $@
MOSTLYCLEANFILES += unilbrk.h unilbrk.h-t

Include:
"unilbrk.h"

